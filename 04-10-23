#include <stdio.h>

int main()
{
    int matriz[3][3];
    int vetor[3];

    for (int i = 0; i < 3; i++)
    {
        for (int j = 0; j < 3; j++)
        {
            printf("Digite os elementos da matriz 3x3:\n");
            scanf("%d", &matriz[i][j]);
        }
    }

    printf("VocÃª digitou a seguinte matriz 3x3:\n");

    vetor[0] = (matriz[0][0] + matriz[0][1] + matriz[0][2]);
    vetor[1] = (matriz[0][1] + matriz[1][1] + matriz[2][1]);
    vetor[2] = (matriz[0][2] + matriz[2][1] + matriz[2][2]);

    for (int i = 0; i < 3; i++)
    {
        printf("%d ", vetor[i]);
    }

    return 0;
}
